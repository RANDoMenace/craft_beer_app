<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<%= form_for(@user) do |user| %>
  <%= user.label :name %>
  <%= user.text_field :name %>
  <%= user.label :email %>
  <%= user.text_field :email %>
  <%= user.label :pic_url %>
  <%= user.text_field :pic_url %>
  <%= user.submit "Submit Beer" %>
<% end %>
